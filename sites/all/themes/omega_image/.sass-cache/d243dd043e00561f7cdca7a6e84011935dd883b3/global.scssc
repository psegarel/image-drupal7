				li 	{	@include background-image(linear-gradient(top, $darkblue, #555));
						margin-left: 1px; 
						
						a 	{	color: #fff;
								padding: 12px 30px;
								text-transform: uppercase;
								border: none;
								font-size: .9em;
								color: #eee;
								
								&:hover	{	@include background-image(linear-gradient(top, #222, $darkblue)); 
											color: #eee;
										}
							}
						
						a.active	{	@include background-image(linear-gradient(top, #555, $darkblue));
										color: #fff;
									}
									
						.last	{	a 	{	border: none;
										}
								}				
					}
					
				li.sf-item-1	{	margin-left: 0; 
									a 	{	border-left: none;
										}
								}	
				}
													
ul.superfish-2	{	.sf-vertical, .sf-vertical li 	{	width: 30em;
													}
				}
				
		
																									
ul#superfish-5		{	

						li  {	a 	{ 	color: #eee;
										padding: 14px 30px;
										text-decoration: none;
										border-left: 1px solid #888;
									}
								a:hover , 
								a.active	{	@include background-image(linear-gradient(#fff, #bbb 20%, #ccc 70%, #ddd));
												color: #222; 
											}	
							}
					}			

